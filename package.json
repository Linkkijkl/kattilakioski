{
  "name": "kattilakioski",
  "version": "0.0.0",
  "description": "Communal kiosk",
  "main": "index.js",
  "repository": "https://github.com/linkkijkl/kattilakioski",
  "author": "Linkki Jyväskylä ry <webmaster@linkkijkl.fi>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "prepare": "yarn smui-theme-light && yarn smui-theme-dark",
    "smui-theme-light": "yarn smui-theme compile frontend/public/smui.css -i frontend/theme",
    "smui-theme-dark": "yarn smui-theme compile frontend/public/smui-dark.css -i frontend/theme/dark"
  },
  "devDependencies": {
    "@mdi/js": "^7.4.47",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tsconfig/svelte": "^5.0.4",
    "smui-theme": "^8.0.0-beta.3",
    "svelte": "^5.15.0",
    "svelte-check": "^4.1.1",
    "typescript": "~5.6.2",
    "vite": "^6.0.5"
  },
  "dependencies": {
    "@smui/drawer": "^8.0.0-beta.3",
    "@smui/icon-button": "^8.0.0-beta.3",
    "@smui/list": "^8.0.0-beta.3",
    "@smui/top-app-bar": "^8.0.0-beta.3"
  }
}
